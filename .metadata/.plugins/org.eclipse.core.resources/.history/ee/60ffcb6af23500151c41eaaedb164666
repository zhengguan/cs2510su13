// some tests on overloading constructor

class AClass {
	int x;
	int y;
	
	AClass() {
		this.x = 0;
		this.y = 0;
	}
	
	AClass(int x, int y) {
		this.x = x;
		this.y = x;
	}
	
//	AClass(Integer x, Integer y) {
//		this.x = y;
//		this.y = y;
//	}
}

class Main{
	public static void main(String[] args) {
		AClass a1 = new AClass();
		AClass a2 = new AClass(1, 2);
		AClass a3 = new AClass(1, new Integer(2));
	}
}