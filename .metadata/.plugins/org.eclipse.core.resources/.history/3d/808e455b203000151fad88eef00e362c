import tester.Tester;

// an XML is a ListOf<XMLFragment>

//REPRESENTS: a XML
interface IXML{}

//REPRESENTS: an empty list of XML fragment
class MTLoXML implements IXML {
	public MTLoXML() {}
}

//REPRESENTS: a non-empty list of XML fragments
class ConsLoXML implements IXML {
	IXMLFragment fst;
	IXML rst;
	
	public ConsLoXML(IXMLFragment fst, IXML rst) {
		this.fst = fst;
		this.rst = rst;
	}
}


// REPRESENTS: a XML fragment
interface IXMLFragment{}

// REPRESENTS: a non tagged XML fragment
class NoTaggedXML implements IXMLFragment {
	
}

// REPRESETNS: a tagged XML fragment
class TaggedXML implements IXMLFragment {
	
}

